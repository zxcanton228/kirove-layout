{
	"name": "kirove-layout",
	"version": "2.0.1",
	"main": "src/main.ts",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/zxcanton228/kirove-layout.git"
	},
	"imports": {
		"src": "./src/*",
		"utils": "./src/utils/*",
		"types": "./src/types/*",
		"core": "./src/core/*"
	},
	"scripts": {
		"start": "ts-node src/main.ts",
		"prod": "node dist/main.js",
		"dev": "ts-node-dev --respawn --transpile-only --require tsconfig-paths/register --exit-child src/main.ts",
		"build:dev": "webpack --env mode=development",
		"build:prod": "webpack --env mode=production"
	},
	"author": {
		"name": "Kirill Vegele",
		"email": "kirillvegele10@gmail.com",
		"url": "https://github.com/zxcanton228"
	},
	"_moduleAliases": {
		"src": "./src",
		"utils": "./src/utils"
	},
	"devDependencies": {
		"@types/inquirer": "^9.0.7",
		"@types/webpack": "^5.28.5",
		"module-alias": "^2.2.3",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.5.4",
		"webpack": "^5.94.0",
		"webpack-cli": "^5.1.4",
		"@types/node": "^22.5.5"
	},
	"dependencies": {
		"app-root-path": "^3.1.0",
		"chalk": "4.1.2",
		"dotenv": "^16.4.5",
		"fs-extra": "^11.2.0",
		"inquirer": "^10.1.8"
	},
	"engines": {
		"node": ">=22.2.0",
		"npm": ">=10.8.0",
		"yarn": ">=1.22.19"
	}
}
